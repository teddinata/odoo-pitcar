<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence untuk Service Booking -->
        <record id="seq_pitcar_service_booking" model="ir.sequence">
            <field name="name">Service Booking Sequence</field>
            <field name="code">pitcar.service.booking</field>
            <field name="prefix">BOOK/%(year)s%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Di file data/sequence.xml -->
        <record id="sequence_cs_leads" model="ir.sequence">
            <field name="name">CS Leads</field>
            <field name="code">cs.leads</field>
            <field name="prefix">LD/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_content_project" model="ir.sequence">
            <field name="name">Content Project Sequence</field>
            <field name="code">content.project</field>
            <field name="prefix">PROJ/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_mentor_request" model="ir.sequence">
            <field name="name">Mentor Request Sequence</field>
            <field name="code">pitcar.mentor.request</field>
            <field name="prefix">MRQ-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- sequence for project management model -->
        <record id="seq_project_management" model="ir.sequence">
            <field name="name">Project Management Sequence</field>
            <field name="code">team.project</field>
            <field name="prefix">PM/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>